{
	"name": "joshuachinemezu/smsglobal-laravel",
	"description": "A Laravel packaage for integrating SmsGlobal",
	"keywords": [
		"php",
		"github",
		"laravel",
		"Open Source",
		"sms",
		"joshuachinemezu",
		"SmsGlobal",
		"Sms Integration",
		"Sms gateway",
		"bulk sms",
		"text messaging",
		"sms marketing",
		"smsglobal",
		"sms api",
		"smsglobal.com",
		"laravel 5"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Joshua Chinemezu",
			"email": "joshuachinemezu@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": "^7.3",
		"illuminate/support": "^5.0|^6.0|^7.0|^8.0",
		"guzzlehttp/guzzle": "5.*|6.*"
	},
	"autoload": {
		"files": [
			"src/Support/helpers.php"
		],
		"psr-4": {
			"JoshuaChinemezu\\SmsGlobal\\": "src/"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"extra": {
		"laravel": {
			"providers": [
				"JoshuaChinemezu\\SmsGlobal\\SmsGlobalServiceProvider"
			],
			"aliases": {
				"SmsGlobal": "JoshuaChinemezu\\SmsGlobal\\Facades\\SmsGlobal"
			}
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^9.2@dev",
		"mockery/mockery": "^1.4@dev",
		"orchestra/testbench": "^6.0@dev"
	}
}
